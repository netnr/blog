{
  //连接字符串
  "ConnectionStrings": {
    "SQLiteConn": "~/data/FileServer.db"
  },

  //安全
  "Safe": {
    //是开发环境，可使用开发环境的接口；警告：正式环境需设为 false
    "IsDev": true,
    //创建App的密码
    "CreateAppPassword": "nr",
    //Token有效期，不能小于Token缓存，单位：分
    "TokenExpired": 30,
    //Token缓存，不能大于Token有效期，单位：分
    "TokenCache": 20
  },

  //静态资源
  "StaticResource": {
    //上传根目录
    "RootDir": "/static/",
    //上传文件大小，单位：MB
    "MaxSize": 100
  }
}