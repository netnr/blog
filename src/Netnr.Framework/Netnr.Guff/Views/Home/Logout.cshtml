@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <title>注销跳转</title>

    <partial name="_PartialQuote" model="@("favicon,global.js")" />
</head>
<body>
    <script>
        guff.fetch(guff.host + "account/logout").then(res => {
            console.log(res)
            location.href = "/"
        }).catch(err => {
            console.log(err);
            location.href = "/"
        })
        
        setTimeout(function () {
            var p = document.createElement('p');
            p.innerHTML = "正在跳转 ...";
            document.body.appendChild(p);
        }, 1000 * 2);
    </script>
</body>
</html>