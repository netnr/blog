@{
    ViewData["Title"] = "draw";

    var mxBaseServer = ViewContext.HttpContext.Request.Headers.Host.ToString().Contains("netnr.eu.org")
    ? "https://s1.netnr.eu.org/libs/mxgraph" : "https://s1.netnr.com/libs/mxgraph";
}

@Html.Raw(Netnr.SharedApp.QuoteTo.Html("loading"))

<input type="hidden" id="mxBaseServer" value='@mxBaseServer' />

<input type="hidden" id="vid" value='@ViewData["vid"]' />

<link href="@mxBaseServer/grapheditor/styles/grapheditor.css" rel="stylesheet" />

<style>
    /*重写菜单样式*/
    html td.mxPopupMenuItem {
        font-size: 12pt;
        padding: 3px 30px;
    }

    /*全屏*/
    .viewFull .geDiagramContainer {
        position: absolute !important;
        top: 0 !important;
        left: 0 !important;
        right: 0 !important;
        z-index: 1;
    }
</style>

<script src="/areas/draw/js/partial/mxg.js" asp-append-version="true"></script>

<script type="text/javascript" src="@mxBaseServer/grapheditor/deflate/pako.min.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/deflate/base64.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/jscolor/jscolor.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/sanitizer/sanitizer.min.js"></script>
<script type="text/javascript" src="@mxBaseServer/mxClient.min.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/EditorUi.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Editor.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Sidebar.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Graph.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Format.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Shapes.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Actions.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Menus.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Toolbar.js"></script>
<script type="text/javascript" src="@mxBaseServer/grapheditor/js/Dialogs.js"></script>